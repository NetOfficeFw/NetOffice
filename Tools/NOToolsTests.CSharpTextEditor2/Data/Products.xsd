<?xml version="1.0" encoding="utf-8"?>
<element name="Person">
  <complexType>
    <sequence>
      <element name="ID" type="System.Int32" readonly="true"/>
      <element name="Name" type="System.String" readonly="false"/>
      <element name="Description" type="System.String" readonly="false"/>
      <element name="Price" type="System.Double" readonly="false"/>
    </sequence>
  </complexType>
</element>
